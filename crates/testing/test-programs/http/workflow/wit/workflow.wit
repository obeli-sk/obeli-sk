package testing:http-workflow;

interface workflow {
    get: func(authority: string, path: string) -> result<string, string>;

    get-successful: func(authority: string, path: string) -> result<string, string>;

    get-successful-concurrently: func(authorities: list<string>) -> result<list<string>, string>;
}

world any {
    import testing:http/http-get;
    import testing:http-obelisk-ext/http-get;
    import my-org:workflow-engine/host-activities;
    export workflow;
}
